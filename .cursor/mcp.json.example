{
  "mcpServers": {
    "taskmaster-ai": {
      "command": "node",
      "args": [
        "/path/to/taskmaster-ai/mcp/index.js"
      ],
      "env": {
        "ANTHROPIC_API_KEY": "your_anthropic_api_key_here",
        "OPENAI_API_KEY": "your_openai_api_key_here",
        "PERPLEXITY_API_KEY": "your_perplexity_api_key_here",
        "GOOGLE_API_KEY": "your_google_api_key_here",
        "MISTRAL_API_KEY": "your_mistral_api_key_here",
        "OPENROUTER_API_KEY": "your_openrouter_api_key_here",
        "XAI_API_KEY": "your_xai_api_key_here",
        "OLLAMA_BASE_URL": "http://localhost:11434/api",
        "OLLAMA_API_KEY": "ollama"
      }
    },
    "github": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "your_github_token_here"
      }
    },
    "browser": {
      "command": "npx",
      "args": [
        "@modelcontextprotocol/server-browser"
      ]
    }
  },
  "security": {
    "notes": [
      "NEVER commit this file with real API keys to version control",
      "Use different API keys for development and production",
      "Regularly rotate your API keys for security",
      "Monitor API usage for unusual activity",
      "Consider using environment variables or secure key management",
      "This template shows the structure - replace all placeholder values"
    ],
    "best_practices": [
      "Keep real mcp.json file out of version control",
      "Use .gitignore and .cursorignore to exclude sensitive configs", 
      "Create separate configurations for different environments",
      "Document which services require which API keys",
      "Test with minimal permissions first"
    ]
  }
} 